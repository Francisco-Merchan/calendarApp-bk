{
  "version": 2,
  "builds": [
    {
      "src": "/index.js",
      "use": "@vercel/node"
    }
  ],
   "rewrites": [
    { "source": "/(.*)", "destination": "/public/index.html" }
  ],
  "headers": [
    { "source": "/(.*)", "headers": [{ "key": "Content-Type", "value": "application/javascript" }] }
  ]
}
