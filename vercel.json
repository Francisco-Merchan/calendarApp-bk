{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    { "handle": "filesystem" },
    { "src": "/api/auth/(.*)", "dest": "/api/auth" },
    { "src": "/api/events/(.*)", "dest": "/api/events" },
    { "src": "/(.*)", "dest": "/index.js" }
  ]
}
